{
  "name": "@chiui/event-cat",
  "type": "module",
  "exports": {
    "import": {
      "types": "./index.d.ts",
      "default": "./esm/index.js"
    },
    "require": {
      "types": "./index.d.ts",
      "default": "./commonjs/index.js"
    }
  },
  "version": "0.0.5",
  "description": "ðŸ˜¼ @chiui/event-cat is a event emitter js library, small, lightweight, and easy to use, support commonjs, esm, umd",
  "author": "chhiuli@gmail.com",
  "homepage": "https://github.com/chiui-li/event-cat-js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chiui-li/event-cat-js.git"
  },
  "bugs": {
    "url": "https://github.com/chiui-li/event-cat-js/issues"
  },
  "license": "MIT",
  "scripts": {
    "dev": "bun run index.ts",
    "build": "npm run build-esm && npm run build-cjs && npm run build-umd",
    "build-esm": "tsc --module es6 --outdir esm",
    "build-cjs": "tsc  --module commonjs --outdir commonjs",
    "build-umd": "tsc  --module umd --outdir umd",
    "test": "bun test"
  },
  "files": [
    "esm",
    "commonjs",
    "umd",
    "index.d.ts",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.7.3"
  },
  "keywords": [
    "event",
    "emitter",
    "event bus",
    "event emitter",
    "event-cat",
    "chiui"
  ],
  "peerDependencies": {}
}
